<p id="notice"><%= notice %></p>

<h1>Listing Posts</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>description</th>
      <th>Rating</th>
      <th>date</th>
      <th>availability</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tdescription>
    <% @posts.each do |post| %>
      <tr>
        <td><%= post.title %></td>
        <td><%= post.description %></td>
        <td><%= post.rating %></td>
        <td><%= post.date %></td>
        <td><%= post.availability %></td>
        <td><%= link_to 'Show', post %></td>
        <td><%= link_to 'Edit', edit_post_path(post) %></td>
        <td><%= link_to 'Destroy', post, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tdescription>
</table>

<br>

<a href="#/post/new">newpost</a>
